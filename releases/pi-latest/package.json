{
	"name": "pijs-web",
	"version": "2.0.1",
	"majorVersion": "2.0",
	"description": "Retro2D graphics, sound, and input library for javascript",
	"types": "pi.d.ts",
	"browser": "dist/pi.js",
	"files": [ "dist" ],
	"exports": {
		".": {
			"browser": "./pi.js",
			"import": "./pi.esm.js",
			"types": "./pi.d.ts"
		},
		"./lite": {
			"browser": "./pi.lite.js",
			"import": "./pi.lite.esm.js",
			"types": "./pi.d.ts"
		},
		"./plugins/gamepad": {
			"browser": "./plugins/gamepad/gamepad.js",
			"import": "./plugins/gamepad/gamepad.esm.js",
			"types": "./pi.d.ts"
		},
		"./plugins/keyboard": {
			"browser": "./plugins/keyboard/keyboard.js",
			"import": "./plugins/keyboard/keyboard.esm.js",
			"types": "./pi.d.ts"
		},
		"./plugins/pointer": {
			"browser": "./plugins/pointer/pointer.js",
			"import": "./plugins/pointer/pointer.esm.js",
			"types": "./pi.d.ts"
		},
		"./plugins/sound": {
			"browser": "./plugins/sound/sound.js",
			"import": "./plugins/sound/sound.esm.js",
			"types": "./pi.d.ts"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AndyStubbs/pijs"
	},
	"author": "Andy Stubbs <andy@pijs.org>",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/AndyStubbs/pijs/issues"
	},
	"homepage": "https://www.pijs.org"
}
